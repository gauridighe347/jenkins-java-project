pipeline {
 agent any
stages{
   stage("checkout"){
     steps {
           sh """
           echo "Checkout done - $PWD"
           ls -l
           """
      }
}
 stage("Building the application"){
     steps {
          sh """
           echo "=======Building the application========"
           mvn clean package
           echo "========building the java application completed========"
          """
           }
      }
  }

}
